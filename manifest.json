{
  "id": "plugin-salesforce",
  "displayName": "Salesforce Publisher",
  "description": "Publisher which pulls data from a Salesforce instance",
  "apiVersion": "2",
  "versionDescription": "Version 1",
  "version": "1.7.2",
  "iconFile": "icon.png",
  "executable": "PluginSalesforce",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "accessGroups": [],
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": true,
  "canConfigure": true,
  "canConfigureRealTime": true,
  "canUseOAuth": true,
  "canWrite": true,
  "querySchema": {
    "ui": {
      "ui:widget": "code",
      "ui:options": {
        "language": "sql"
      }
    },
    "schema": {
      "type": "string"
    }
  },
  "configSchema": {
    "ui": {
      "ui:order": [
        "TlsVersion"
      ]
    },
    "schema": {
      "type": "object",
      "properties": {
        "TlsVersion": {
          "type": "string",
          "title": "TLS Version",
          "description": "Which TLS version to use in the plugin",
          "default": "TLS 1.2",
          "enum": [
            "TLS 1.2",
            "TLS 1.3"
          ]
        }
      },
      "required": [
        "TlsVersion"
      ]
    }
  }
}